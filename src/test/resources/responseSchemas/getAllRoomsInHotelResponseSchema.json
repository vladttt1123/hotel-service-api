{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "minimum": 1
      },
      "roomNumber": {
        "type": "integer",
        "minimum": 1
      },
      "roomType": {
        "type": "string",
        "enum": [
          "SINGLE",
          "DOUBLE",
          "SUITE"
        ]
      },
      "available": {
        "type": "boolean"
      }
    },
    "required": [
      "id",
      "roomNumber",
      "roomType",
      "available"
    ],
    "additionalProperties": false
  }
}